{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/gaihcc2dataserver/bin/Debug/net8.0/gaihcc2dataserver.dll",
            "env": {
                "CLIENT": "Client1X",
                "API_HOST": "192.168.1.41",
                "API_SUFFIX": "api/v1",
                "API_PROTOCOL": "http",
                "API_PORT": "5236",
                "TIME_PERIOD": "10",
                "ON_TIME": "20000",
                "OFF_TIME": "0",
                "LOG_LEVEL": "info",
                "INTERLEAVE": "1",
                "TREND_PORT": "5001",
                "TREND_PORT_ANYIP": "5001",
                "DOTNET_HOSTBUILDER__RELOADCONFIGONCHANGE": "false",
                "TREND_URL":"http://localhost:5001"
            },
            "args": [],
            "cwd": "${workspaceFolder}/gaihcc2dataserver",
            "console": "internalConsole",
            "stopAtEntry": false,
            "logging":{
                "moduleLoad": false,
                "engineLogging": false,
                "exceptions": false
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}