    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/mtapiclientcs/bin/Debug/net8.0/mtapiclientcs.dll",
            "env": {
                "CLIENT": "Client1",
                "API_HOST": "localhost",
                "API_SUFFIX": "api/v1",
                "API_PROTOCOL": "http",
                "API_PORT": "5236",
                "TIME_PERIOD": "10",
                "WH_HOST": "localhost",
                "WH_SUFFIX": "api/v1",
                "WH_PROTOCOL": "http",
                "WH_PORT": "8002"
            },
            "args": [],
            "cwd": "${workspaceFolder}/mtapiclientcs",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}